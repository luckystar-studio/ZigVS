{
  "comments": {
    "lineComment": "//"
  },
  "brackets": [
    [ "{", "}" ],
    [ "[", "]" ],
    [ "(", ")" ]
  ],
  "colorizedBracketPairs": [
    [ "{", "}" ],
    [ "[", "]" ],
    [ "(", ")" ]
  ],
  "autoClosingPairs": [
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "{",
      "close": "}"
    },
    {
      "open": "\"",
      "close": "\"",
      "notIn": [ "string" ]
    },
    {
      "open": "'",
      "close": "'",
      "notIn": [ "string" ]
    }
  ],
  "autoCloseBefore": ";:.,=}])> \n\t",
  "surroundingPairs": [
    [ "{", "}" ],
    [ "[", "]" ],
    [ "(", ")" ],
    [ "<", ">" ],
    [ "\"", "\"" ],
    [ "'", "'" ],
    [ "`", "`" ],
    [ "```", "```" ]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^.*\\{[^}\"']*$|^.*\\([^\\)\"']*$",
    "decreaseIndentPattern": "^\\s*(\\s*\\/[*].*[*]\\/\\s*)*[})]"
  },
  "folding": {
    "markers": {
      "start": "^\\s*// region:\\b",
      "end": "^\\s*// endregion\\b"
    }
  },
  "wordPattern": "[a-zA-Z_][a-zA-Z0-9_]*"
}